<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss} [%highlight(%-5level)] %logger %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="org.jline" level="ERROR"/>
    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
